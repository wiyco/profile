{
  "name": "wiyco-profile",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postinstall": "npm run db:gen",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export",
    "eslint:fix": "eslint ./src --ext .js,jsx,.ts,.tsx --fix",
    "stylelint:fix": "stylelint ./src/**/*.{css,scss} --fix",
    "prettier:fix": "prettier --check --write './src/**/*.{js,jsx,ts,tsx,css,scss,md,mdx}'",
    "format": "npm run eslint:fix && npm run stylelint:fix && npm run prettier:fix",
    "db:studio": "dotenv -e .env.local prisma studio",
    "db:intro": "dotenv -e .env.local prisma introspect",
    "db:pull": "dotenv -e .env.local prisma db pull",
    "db:gen": "prisma generate",
    "db:push": "dotenv -e .env.local prisma db push",
    "db:migrate:dev": "dotenv -e .env.local prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.local prisma migrate deploy"
  },
  "dependencies": {
    "@nextui-org/react": "^2",
    "@prisma/client": "^5",
    "@react-three/fiber": "^8",
    "@react-three/postprocessing": "^2",
    "@supabase/ssr": "^0",
    "@supabase/supabase-js": "^2",
    "@vercel/analytics": "^1.2.2",
    "clsx": "^2",
    "date-fns": "^3",
    "framer-motion": "^10",
    "leva": "^0",
    "maath": "^0",
    "meshline": "^3",
    "next": "14.0.4",
    "next-themes": "^0",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^9",
    "rehype-raw": "^7",
    "tailwind-merge": "^2",
    "tailwindcss": "^3",
    "three": "^0",
    "zod": "^3",
    "zod-prisma-types": "^3"
  },
  "devDependencies": {
    "@svgr/webpack": "^8",
    "@types/node": "^20.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/three": "^0",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "autoprefixer": "^10",
    "dotenv-cli": "^7",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9",
    "eslint-plugin-import": "^2",
    "eslint-plugin-simple-import-sort": "^10",
    "eslint-plugin-unused-imports": "^3",
    "postcss": "^8",
    "prettier": "^3",
    "prettier-plugin-tailwindcss": "^0",
    "prisma": "^5",
    "sass": "^1",
    "stylelint": "^15",
    "stylelint-config-recess-order": "^4",
    "stylelint-config-standard": "^34",
    "stylelint-config-standard-scss": "^11",
    "stylelint-scss": "^5",
    "typescript": "^5"
  }
}
